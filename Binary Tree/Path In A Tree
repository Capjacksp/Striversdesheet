void path(TreeNode<int>*root, vector<int>v, vector<int>&ans, int x){
    if(root==NULL)return ;
    v.push_back(root->data);
    if(root->data==x)ans=v;
    path(root->left,v,ans,x);
    path(root->right,v,ans,x);
}


vector<int> pathInATree(TreeNode<int> *root, int x)
{
    vector<int>v,ans;
    path(root,v,ans,x);
    return ans;
}



